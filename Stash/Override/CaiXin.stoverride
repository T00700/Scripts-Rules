name: 财新会员
desc: 解锁财新文章阅读与音频限制
http:
  mitm:
    - 'gateway.caixin.com'
  script:
    - match: ^https?:\/\/gateway\.caixin\.com\/api\/app\-api\/auth\/(validate|validateAudioAuth)
      name: CaiXin
      type: request
      require-body: false
      timeout: 10
script-providers:
  CaiXin:
    url: https://raw.githubusercontent.com/Centralmatrix3/Scripts-Rules/Master/Stash/Script/CaiXin.Js
    interval: 86400
